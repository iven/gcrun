[Environment]
Env_Terminal = gnome-terminal --use-factory
Env_TermExec = ${Terminal} -e "%s"

[Geometry]
Geo_Width = 400
Geo_Top = 100
Geo_Left = 200

[History]
His_Size = 256

[Completion]

[Switch]
Swi_ShowLastHistory = true
Swi_ShowDotFiles = true

[Terminal]
Ter_Always = ssh;telnet;ftp;lynx;mc;vi;vim;pine;centericq;perldoc;man;

[Protocol]
Pro_http = http;https;
Pro_file = file;
Pro_mailto = mailto;
Pro_manual = man;
Pro_info = info;
Pro_readme = rm;
Pro_shell = sh;

[ProtocolHandler]
H_Pro_http = firefox %u
H_Pro_file = nautilus %u
H_Pro_mailto = mozilla -remote "mailto(%s)"
H_Pro_manual = man %s
H_Pro_info = info %s
H_Pro_readme = less %s
H_Pro_shell = sh -c '%s'

[Extension]
Ext_Text = txt;cc;cpp;h;java;html;htm;epl;tex;latex;js;css;xml;xsl;am;
Ext_Document = doc;rtf;
Ext_Pdf = pdf;

[ExtensionHandler]
H_Ext_Text = gedit %s
H_Ext_Document = AbiWord %s
H_Ext_Pdf = evince %s
