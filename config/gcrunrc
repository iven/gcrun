[Environment]
Env_Terminal = gnome-terminal --start-factory-server --use-factory
Env_TermExec = ${Terminal} -e

[Geometry]
Geo_Width = 400
Geo_Top = 100
Geo_Left = 200

[History]
His_Size = 256

[Completion]
Com_TimeOut = 1000

[Switch]
Swi_ShowLastHistory = true
Swi_ShowDotFiles = true
Swi_AutoCompletion = true

[Terminal]
Ter_Always = ssh;telnet;ftp;lynx;mc;vi;vim;pine;centericq;perldoc;man;

[Protocol]
Pro_http = http;https;
Pro_file = file;
Pro_mailto = mailto;
Pro_manual = man;
Pro_info = info;
Pro_readme = rm;
Pro_shell = sh;

[ProtocolHandler]
PH_http = firefox %u
PH_mailto = mozilla -remote "mailto(%s)"
PH_manual = man %s
PH_info = info %s
PH_readme = less %s
PH_shell = sh -c '%s'

[Extension]
Ext_Text = txt;cc;cpp;h;java;html;htm;epl;tex;latex;js;css;xml;xsl;am;
Ext_Document = doc;rtf;
Ext_Pdf = pdf;

[ExtensionHandler]
EH_Text = gedit %s
EH_Document = AbiWord %s
EH_Pdf = evince %s
